        JsonNodeFactory factory = new JsonNodeFactory(false);
        // Create a new ArrayNode
        ArrayNode newArrayNode = new ArrayNode(factory);

        // Traverse old ArrayNode
        for (JsonNode node : arrayNode) {
            // Create a new ObjectNode
            ObjectNode newObjectNode = factory.objectNode();
            // Traverse each field in the object
            node.fields().forEachRemaining(field -> {
                String stringValue = field.getValue().asText(); // Convert value to string
                newObjectNode.put(field.getKey(), stringValue);
            });
            newArrayNode.add(newObjectNode);
        }

        // Now you can use your newArrayNode for the csv conversion
        System.out.println(newArrayNode);
    }